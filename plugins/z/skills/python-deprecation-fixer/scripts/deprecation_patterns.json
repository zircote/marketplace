{
  "patterns": [
    {
      "name": "datetime_utcnow",
      "category": "datetime",
      "deprecated": "datetime.utcnow()",
      "replacement": "datetime.now(UTC)",
      "regex": "datetime\\.utcnow\\(\\)",
      "replacement_template": "datetime.now(UTC)",
      "required_imports": ["from datetime import UTC"],
      "python_version": "3.12+",
      "description": "datetime.utcnow() is deprecated in favor of timezone-aware datetime.now(UTC)"
    },
    {
      "name": "datetime_utcnow_factory",
      "category": "datetime",
      "deprecated": "default_factory=datetime.utcnow",
      "replacement": "default_factory=lambda: datetime.now(UTC)",
      "regex": "default_factory\\s*=\\s*datetime\\.utcnow(?!\\()",
      "replacement_template": "default_factory=lambda: datetime.now(UTC)",
      "required_imports": ["from datetime import UTC"],
      "python_version": "3.12+",
      "description": "Pydantic Field default_factory should use timezone-aware datetime"
    },
    {
      "name": "datetime_utcfromtimestamp",
      "category": "datetime",
      "deprecated": "datetime.utcfromtimestamp()",
      "replacement": "datetime.fromtimestamp(*, UTC)",
      "regex": "datetime\\.utcfromtimestamp\\(([^)]+)\\)",
