{
  "name": "epicpast-marketplace",
  "owner": {
    "name": "epic pastures",
    "email": "bob@epicpastures.com",
    "url": "https://github.com/epicpast"
  },
  "metadata": {
    "description": "NSIP sheep breeding data tools and MCP integrations for Claude Code",
    "version": "1.3.0"
  },
  "plugins": [
    {
      "name": "nsip",
      "source": "./plugins/nsip",
      "description": "Access NSIP sheep breeding data with MCP tools and slash commands",
      "version": "1.3.0",
      "author": {
        "name": "epic pastures",
        "email": "bob@epicpastures.com",
        "url": "https://github.com/epicpast"
      },
      "license": "MIT",
      "keywords": ["nsip", "sheep", "breeding", "MCP", "claude-code"],
      "strict": false,
      "commands": [
        "./commands/consult.md",
        "./commands/discover.md",
        "./commands/health.md",
        "./commands/lineage.md",
        "./commands/lookup.md",
        "./commands/profile.md",
        "./commands/progeny.md",
        "./commands/search.md",
        "./commands/test-api.md",
        "./commands/traits.md"
      ],
      "agents": [
        "./agents/shepherd.md"
      ],
      "mcpServers": [
        "./.mcp.json"
      ],
      "hooks": {
        "PreToolUse": [
          {
            "matcher": "mcp__nsip__nsip_get_animal",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/lpn_validator.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/trait_dictionary.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_search_by_lpn",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/lpn_validator.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/trait_dictionary.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_lineage",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/lpn_validator.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_progeny",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/lpn_validator.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_search_animals",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/breed_context_injector.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/trait_dictionary.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_trait_ranges",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/breed_context_injector.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/trait_dictionary.py"
              }
            ]
          }
        ],
        "PostToolUse": [
          {
            "matcher": "mcp__nsip__.*",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/auto_retry.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/error_notifier.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_animal",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/fallback_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/query_logger.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/result_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/breeding_report.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_search_by_lpn",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/fallback_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/query_logger.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/result_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/breeding_report.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_search_animals",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/fallback_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/query_logger.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/csv_exporter.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_lineage",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/fallback_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/query_logger.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/result_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/pedigree_visualizer.py"
              }
            ]
          },
          {
            "matcher": "mcp__nsip__nsip_get_progeny",
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/fallback_cache.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/query_logger.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/result_cache.py"
              }
            ]
          }
        ],
        "SessionStart": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/api_health_check.py"
              }
            ]
          }
        ],
        "UserPromptSubmit": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/smart_search_detector.py"
              },
              {
                "type": "command",
                "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/comparative_analyzer.py"
              }
            ]
          }
        ]
      }
    },
    {
      "name": "z",
      "source": "./plugins/z",
      "description": "Comprehensive agent library featuring 100+ specialized Opus 4.5 agents organized by domain",
      "version": "1.0.0",
      "author": {
        "name": "zircote",
        "url": "https://github.com/zircote"
      },
      "license": "MIT",
      "keywords": ["agents", "opus-4.5", "development", "skills", "commands", "specialists", "subagents"],
      "strict": false,
      "agents": [
        "./agents/01-core-development/*.md",
        "./agents/02-language-specialists/*.md",
        "./agents/03-infrastructure/*.md",
        "./agents/04-quality-security/*.md",
        "./agents/05-data-ai/*.md",
        "./agents/06-developer-experience/*.md",
        "./agents/07-specialized-domains/*.md",
        "./agents/08-business-product/*.md",
        "./agents/09-meta-orchestration/*.md",
        "./agents/10-research-analysis/*.md",
        "./agents/templates/*.md"
      ],
      "skills": [
        "./skills/*/SKILL.md"
      ],
      "commands": [
        "./commands/*.md",
        "./commands/code/*.md"
      ]
    },
    {
      "name": "git",
      "source": "./plugins/git",
      "description": "Git workflow commands for commits, pushes, PRs, and syncing, plus GitHub ecosystem integration skills",
      "version": "1.0.0",
      "author": {
        "name": "zircote",
        "url": "https://github.com/zircote"
      },
      "license": "MIT",
      "keywords": ["git", "github", "version-control", "adr", "workflows", "ci-cd", "commits"],
      "strict": false,
      "skills": [
        "./adr/skills/SKILL.md",
        "./ecosystem/skills/SKILL.md"
      ],
      "commands": [
        "./commands/cm.md",
        "./commands/cp.md",
        "./commands/ff.md",
        "./commands/fr.md",
        "./commands/pr.md",
        "./commands/prune.md",
        "./commands/sync.md"
      ]
    },
    {
      "name": "datadog",
      "source": "./plugins/datadog",
      "description": "DataDog API specialists for monitoring setup, dashboard creation, metric queries, and APM integration",
      "version": "1.0.0",
      "author": {
        "name": "zircote",
        "url": "https://github.com/zircote"
      },
      "license": "MIT",
      "keywords": ["datadog", "monitoring", "observability", "apm", "metrics", "dashboards", "alerts"],
      "strict": false,
      "agents": [
        "./agents/datadog-pro.md",
        "./agents/datadog-api-expert.md"
      ]
    },
    {
      "name": "copilot",
      "source": "./plugins/copilot",
      "description": "GitHub Copilot coding agent onboarding tools for configuring repositories with copilot-instructions.md",
      "version": "1.0.0",
      "author": {
        "name": "zircote",
        "url": "https://github.com/zircote"
      },
      "license": "MIT",
      "keywords": ["copilot", "github", "onboarding", "ai-agents", "configuration"],
      "strict": false,
      "agents": [
        "./onboard/agents/onboard.md"
      ],
      "commands": [
        "./onboard/commands/onboard.md"
      ]
    },
    {
      "name": "document-skills",
      "source": "./plugins/document-skills",
      "description": "Document processing skills for PDF, DOCX, XLSX, and PPTX file manipulation with AI-powered analysis",
      "version": "1.0.0",
      "author": {
        "name": "zircote",
        "url": "https://github.com/zircote"
      },
      "license": "MIT",
      "keywords": ["documents", "pdf", "docx", "xlsx", "pptx", "office", "extraction", "analysis"],
      "strict": false,
      "skills": [
        "./pdf/SKILL.md",
        "./docx/SKILL.md",
        "./xlsx/SKILL.md",
        "./pptx/SKILL.md"
      ]
    }
  ]
}
